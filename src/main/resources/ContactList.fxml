<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<Scene xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minWidth="350.0">
      <MenuBar maxWidth="1.7976931348623157E308" VBox.vgrow="NEVER">
         <Menu mnemonicParsing="false" text="File">
            <MenuItem mnemonicParsing="false" text="Logout" />
         </Menu>
      </MenuBar>
      <GridPane hgap="15.0" vgap="5.0" VBox.vgrow="NEVER">
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
         </rowConstraints>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
         <Pane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="60.0" prefWidth="60.0" style="-fx-border-color: lightgray; -fx-border-radius: 4;" GridPane.rowSpan="2">
            <children>
               <ImageView id="profile_picture" fitHeight="64.0" fitWidth="64.0" pickOnBounds="true" preserveRatio="true">
                  <image>
                     <Image url="@blank-profile.png" />
                  </image></ImageView>
            </children>
         </Pane>
         <TextField id="nickname" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" />
         <TextField id="status" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="1" />
      </GridPane>
      <Separator prefWidth="200.0" />
      <TextField id="contacts_filter" promptText="Search for contacts...">
         <VBox.margin>
            <Insets bottom="5.0" left="15.0" right="15.0" top="5.0" />
         </VBox.margin>
      </TextField>
      <ListView id="contactList" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS" />
   </VBox>
</Scene>
