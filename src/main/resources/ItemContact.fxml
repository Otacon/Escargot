<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>

<HBox spacing="5.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <ImageView id="status" fitHeight="10.0" fitWidth="10.0" pickOnBounds="true" preserveRatio="true">
        <HBox.margin>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
        </HBox.margin>
        <Image url="@status-online.png" />
    </ImageView>
    <Label id="nick" alignment="CENTER" maxHeight="1.7976931348623157E308" text="Nick" textFill="black" />
    <Label id="personal_message" alignment="CENTER" maxHeight="1.7976931348623157E308" text="Status" textFill="gray" />
</HBox>
